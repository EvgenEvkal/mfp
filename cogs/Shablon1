using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace kait.cogs
{
    internal class Shablon1
    {
        public static void Program(string[] args)
        {
            int n = 5;
            for (int i = 1; i <= 10; i++)
            {
                Console.WriteLine($"5 * {i} = {n * i}");
            }
        }
    }
}
